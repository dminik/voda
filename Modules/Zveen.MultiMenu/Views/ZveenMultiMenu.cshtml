@using Orchard;
@using Orchard.Core;
@using Orchard.Mvc.Extensions;
@using Orchard.Mvc;

@{
    var menu = (Zveen.MultiMenu.ViewModels.MultiMenuFrontendViewModel)Model.Menu; 
}

@using (Script.Foot())
{
    @Html.Raw(menu.Include)
    
    <script type="text/javascript">
        $().ready(function () {
            @Html.Raw(menu.Script)
        });  
    </script>
    
}

@Html.Raw(menu.BeforeMenu)
    @menu.RenderMenu(menu, menu.MenuItems,1)
@Html.Raw(menu.AfterMenu)
